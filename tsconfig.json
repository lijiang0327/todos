{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "noEmit": true,
      "strict": true,
      "moduleResolution": "NodeNext",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "jsx": "react",
    },
    "include": ["src"]
  }